<div class="row">
  <%= form_with(model: @article, class:"col s12") do |f| %>
    <div class="row">
      <div class="col s6">
        <%= f.text_field :title, placeholder: "タイトル" %>
      </div>
      <div class="input-field col s8">
        <%= f.text_area :text, class:"materialize-textarea", placeholder: "本文" %>
      </div>
      <div class="col s12">
        <%= f.submit "送信" %>
      </div>
    </div>
  <% end %>
</div>
